[*.cs]

# Suppressions

# Remove the underscores from member name
dotnet_diagnostic.CA1707.severity = none

# Prefer 'static readonly' fields over constant array arguments if the called method is called repeatedly and is not mutating the passed array
dotnet_diagnostic.CA1861.severity = none

# In externally visible method, validate parameter is non-null before using it. If appropriate, throw an 'ArgumentNullException' when the argument is 'null'
dotnet_diagnostic.CA1062.severity = none